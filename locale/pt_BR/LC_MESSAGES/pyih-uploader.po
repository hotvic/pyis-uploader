#: pyih-uploader.py:25 pyih-uploader.py:29
msgid "HELPMSG"
msgstr ""
"Uso: pyih-uploader [opções] nome_do_arquivo\n"
"Opções:\n"
"  -P|--pass pass  :      Sua senha (Envia para sua conta, sem cookie)\n"
"  -U|--user user  :      Seu usuário (Envia para sua conta, sem cookie)\n"
"  -c|--cookie id  :      Usar Código de Registro (Envia para sua conta)\n"
"  -r|--resize WxH :      Redimensionar imagem\n"
"  -K|--clipboard  :      Enviar URl da imagem para Área de Transferência\n"
"  -t|--thb-only   :      Somente mostra a URL da imagem (Miniatura)\n"
"  -u|--url-only   :      Somente mostra a URL da imagem (Completa)\n"
"  -v|--verbose    :      Exibir mensagens de depuração\n"
"  -V|--version    :      Exibir a versão do programa e sair\n"
"  -h|--help       :      Exibir essa ajuda e sair"

#: pyih-uploader.py:38
msgid "Missing Password"
msgstr "Você precisa especificar uma senha (-P|--pass)"

#: pyih-uploader.py:41
msgid "Missing Username"
msgstr "Você precisa especificar um usuário (-U|--user)"

#: pyih-uploader.py:54
msgid "Open File Error"
msgstr "Erro ao abrir arquivo para leitura, tem certeza que o arquivo existe ?"

#: pyih-uploader.py:63
msgid "XML Parse Error"
msgstr "Falha ao enviar imagem, tente novamente!"

#: pyih-uploader.py:101 pyih-uploader.py:108 pyih-uploader.py:124
msgid "You need PyGTK to send to clipboard"
msgstr "Você precisa de PyGTK para enviar para área de transferência"

